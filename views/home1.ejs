    <script>
    document.addEventListener('DOMContentLoaded', function() {
    var header = document.getElementById('header');
    var links = document.querySelectorAll('.dark-link');

    window.addEventListener('scroll', function() {
        if (window.scrollY > 0) {
            header.classList.add('transparent');

            // Add a class to the anchor tags to change their color when scrolled
            links.forEach(function(link) {
                link.classList.add('scrolled-link');
            });
        } else {
            header.classList.remove('transparent');

            // Remove the class to revert the color when scrolled to the top
            links.forEach(function(link) {
                link.classList.remove('scrolled-link');
            });
        }
    });
});


document.addEventListener('DOMContentLoaded', function () {
    // Add a class to trigger the transition on page load
    document.querySelector('.heading').classList.add('show');

    setTimeout(function () {
        document.querySelector('.subheading').classList.add('show');
    }, 500);
});

document.addEventListener('DOMContentLoaded', function() {
    var avatarHeading = document.getElementById('avatarHeading');
    var servicesHeading = document.getElementById('servicesHeading');
  
    function handleIntersection(entries, observer) {
      entries.forEach(function(entry) {
        if (entry.isIntersecting) {
          if (entry.target === avatarHeading) {
            avatarHeading.style.transform = 'translateX(0)'; // Move to the original position
          } else if (entry.target === servicesHeading) {
            servicesHeading.style.transform = 'translateX(0)'; // Move to the original position
          }
        } else {
          if (entry.target === avatarHeading) {
            avatarHeading.style.transform = 'translateX(-100%)'; // Move outside the viewport
          } else if (entry.target === servicesHeading) {
            servicesHeading.style.transform = 'translateX(100%)'; // Move outside the viewport
          }
        }
      });
    }
  
    var observer = new IntersectionObserver(handleIntersection);
  
    observer.observe(avatarHeading);
    observer.observe(servicesHeading);
  });
  

// ready playr me api code

const subdomain = "demo";
const frame = document.getElementById("iframe");

frame.src = `https://${subdomain}.readyplayer.me/avatar?frameApi`;

window.addEventListener("message", subscribe);
document.addEventListener("message", subscribe);

function subscribe(event) {
  const json = parse(event);

  if (json?.source !== "readyplayerme") {
    return;
  }

  // Susbribe to all events sent from Ready Player Me once frame is ready
  if (json.eventName === "v1.frame.ready") {
    frame.contentWindow.postMessage(
      JSON.stringify({
        target: "readyplayerme",
        type: "subscribe",
        eventName: "v1.**",
      }),
      "*"
    );
  }

  // Get avatar GLB URL
  if (json.eventName === "v1.avatar.exported") {
    console.log(`Avatar URL: ${json.data.url}`);
    document.getElementById("avatar").value = `${json.data.url}`;
    document.getElementById("iframe").hidden = true;
  }

  // Get user id
  if (json.eventName === "v1.user.set") {
    console.log(
      `User with id ${json.data.id} set: ${JSON.stringify(json)}`
    );
  }
}

function parse(event) {
  try {
    return JSON.parse(event.data);
  } catch (error) {
    return null;
  }
}


function displayIframe() {
  let iframe = document.getElementById('iframe');
  let button = document.getElementById('displayButton');
  if (iframe.style.display === 'block') {
    iframe.style.display = 'none';
    button.textContent = 'Open Avatar Creator';
  } else {
    iframe.style.display = 'block';
    button.textContent = 'Close Avatar Creator';
  }
}

document.addEventListener('DOMContentLoaded', function() {
  var avatarForm = document.getElementById('avatarform');
  var Wrapper = document.getElementById('wrapper');

  function handleIntersection(entries, observer) {
      entries.forEach(function(entry) {
          if (entry.isIntersecting) {
               if (entry.target === avatarForm) {
                  avatarForm.style.transform = 'translateX(0)'; // Move to the original position
              }else if (entry.target === Wrapper) {
                Wrapper.style.transform = 'translateX(0)'; // Move to the original position
              }
          } else {
              if (entry.target === avatarForm) {
                  avatarForm.style.transform = 'translateX(100%)'; // Move outside the viewport
              }else if (entry.target === Wrapper) {
                Wrapper.style.transform = 'translateX(-100%)'; // Move to the original position
              }
          }
      });
  }

  var observer = new IntersectionObserver(handleIntersection);

  observer.observe(avatarForm);
  observer.observe(Wrapper);
});

