.navbar-right {
    display: flex;
    align-items: center;
    gap: 10px;
}

.loginbtn {
    border: none;
    outline: none;
    background: transparent;
    cursor: pointer;
}

.navbar.scrolled .navlinks, .navbar.scrolled .loginbtn {
    font-weight: 500;
    font-size: 16px;
}

.navlinks, .loginbtn {
    position: relative;
    text-decoration: none;
    color: black;
    font-weight: 400;
    font-size: 14px;
}

.navlinks::after, .loginbtn::after {
    content: "";
    position: absolute;
    left: 0;
    bottom: -2px;
    width: 100%;
    height: 2px;
    background-color: transparent;
    transition: background-color 0.8s ease;
    box-sizing: border-box;
}

.navlinks:hover::after, .loginbtn:hover::after {
    background-color: #e91e63;
}

.navlinks::before, .loginbtn::before {
    content: "";
    position: absolute;
    left: 0;
    bottom: -2px;
    width: 0;
    height: 2px;
    background-color: #e91e63;
    transition: width 0.8s ease;
    box-sizing: border-box;
}

.navlinks:hover::before, .loginbtn:hover::before {
    width: 100%;
}

/* Hamburger styles */
